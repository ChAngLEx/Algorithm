# Binary äºŒåˆ†
0class Solution:
    def search(self, nums: List[int], target: int) -> int:
        if not nums:
            return -1
    
        start = 0
        end = len(nums) - 1
        while (start + 1 < end):
            mid = (start + end) // 2
            if (target == nums[mid]):
                return mid
           
            if (nums[start] > nums[mid]):
                if (target > nums[mid] and target < nums[start]):
                    start = mid
                else:
                    end = mid

            else:
                if (target < nums[mid] and target > nums[start]):
                    end = mid
                else:
                    start = mid
        
        if (nums[0] == target):
            return 0
        if (nums[start] == target):
            return start
        if (nums[end] == target):
            return end
        return -1
